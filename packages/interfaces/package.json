{
	"name": "@acai/interfaces",
	"version": "0.2.0-beta.11",
	"description": "General use typescript interfaces for use across all the Açaí modules.",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AcaiJS/modules.git",
		"directory": "packages/interfaces"
	},
	"author": "Rafael <rafael.correa@aposoftworks.com>",
	"license": "BSD-3-Clause",
	"keywords": [
		"typescript",
		"interfaces",
		"types",
		"acai",
		"modules"
	],
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*"
			]
		}
	},
	"private": false,
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "tsup",
		"lint": "eslint .",
		"lint:fix": "eslint --quiet **/* --fix",
		"prepublishOnly": "yarn build",
		"test": "",
		"audit": "yarn audit"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"devDependencies": {
		"@types/cookies": "^0.7.6",
		"@types/node": "^14.14.28",
		"@typescript-eslint/eslint-plugin": "~5.5.0",
		"@typescript-eslint/parser": "~5.5.0",
		"eslint": "~8.4.0",
		"tsup": "~5.11.4",
		"typescript": "^4.1.5",
		"ts-node-dev": "~1.1.8"
	}
}
