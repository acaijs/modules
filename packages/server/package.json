{
	"name": "@acai/server",
	"version": "3.0.0-beta.45",
	"description": "The base server for the a√ßai framework",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AcaiJS/modules.git",
		"directory": "packages/server"
	},
	"keywords": [
		"server",
		"http",
		"middleware",
		"provider",
		"typescript",
		"acai",
		"module"
	],
	"author": "Rafael <rafael.chaves98@hotmail.com>",
	"license": "BSD-3-Clause",
	"files": [
		"dist"
	],
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*"
			]
		}
	},
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./http": {
			"import": "./dist/http.mjs",
			"require": "./dist/http.js"
		}
	},
	"private": false,
	"scripts": {
		"build": "tsup",
		"test": "ts-node-dev ./test.ts --transpile-only",
		"lint": "eslint .",
		"prepublishOnly": "yarn build",
		"audit": "yarn audit"
	},
	"devDependencies": {
		"@acai/interfaces": "^0.2.0-beta.0",
		"@acai/testing": "^3.0.0-beta.0",
		"@types/cookies": "^0.7.6",
		"@types/node": "^14.14.28",
		"@types/supertest": "^2.0.11",
		"@typescript-eslint/eslint-plugin": "~5.5.0",
		"@typescript-eslint/parser": "~5.5.0",
		"eslint": "~8.4.0",
		"form-data": "^4.0.0",
		"supertest": "^6.1.6",
		"ts-node-dev": "~1.1.8",
		"tsup": "~5.11.4",
		"typescript": "^4.1.5"
	},
	"dependencies": {
		"@acai/router": "^2.2.0-beta.0",
		"@acai/utils": "^0.7.0-beta.0",
		"body-parser": "^1.19.0",
		"cookies": "^0.8.0"
	}
}
